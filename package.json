{
  "name": "slidev-addon-d2-diagrams",
  "version": "0.2.0",
  "type": "module",
  "description": "Slidev addon for rendering D2 diagrams with theme support, dark mode sync, and animations",
  "keywords": [
    "slidev-addon",
    "slidev",
    "d2",
    "d2lang",
    "diagrams",
    "presentation",
    "slides"
  ],
  "author": "ndisidore",
  "license": "MIT",
  "homepage": "https://github.com/ndisidore/slidev-addon-d2-diagrams",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndisidore/slidev-addon-d2-diagrams.git"
  },
  "bugs": {
    "url": "https://github.com/ndisidore/slidev-addon-d2-diagrams/issues"
  },
  "files": [
    "components",
    "vite.config.ts"
  ],
  "scripts": {
    "dev": "slidev --open",
    "build": "slidev build",
    "export": "slidev export",
    "preview": "slidev build && slidev preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "npm run format:check && npm run tsc:check && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsc:check": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@terrastruct/d2": "^0.1.33"
  },
  "peerDependencies": {
    "@slidev/cli": ">=52.0.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@slidev/cli": "^52.11.1",
    "@slidev/theme-default": "^0.25.0",
    "@slidev/types": "^52.11.1",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^14.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "jsdom": "^27.3.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vue": "^3.5.26"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
